# System-wide .profile for sh(1)

if [ -x /usr/libexec/path_helper ]; then
	eval `/usr/libexec/path_helper -s`
fi

if [ "${BASH-no}" != "no" ]; then
	[ -r /etc/bashrc ] && . /etc/bashrc
fi

# ADD RUSH TO PATH, IF NOT ALREADY
#    This /looks/ like it might be csh syntax, but it's not.
#
if ( /bin/echo "$PATH" | grep /usr/local/rush > /dev/null ); then sleep 0
else PATH="${PATH}:/usr/local/rush/bin"; export PATH; fi


# BEGIN HARMONY PATH REGISTRATION
export HARMONY_PATH="/Applications/Toon Boom Harmony 9.2/tba"
export HARMONY_BIN_PATH="/Applications/Toon Boom Harmony 9.2/tba/macosx/bin"
export PATH="$PATH:$HARMONY_BIN_PATH"
# END HARMONY PATH REGISTRATION
